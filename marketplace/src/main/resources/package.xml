<package type="addon" name="microsoft-azure-online-storage" version="@VERSION@">
  <title>Microsoft Azure Online Storage</title>
  <description>
    The Microsoft Azure Online Storage addon is a Nuxeo Binary Manager that stores binaries in an Azure Storage container, with ability to plug to the Azure CDN service.
  </description>
  <home-page>https://doc.nuxeo.com/nxdoc/microsoft-azure-online-storage/</home-page>
  <platforms>
    <platform>server-@DISTRIB_VERSION@</platform>
    <platform>server-@DISTRIB_VERSION@-*</platform>
  </platforms>
  <hotreload-support>false</hotreload-support>
  <supported>true</supported>
  <require-terms-and-conditions-acceptance>true</require-terms-and-conditions-acceptance>
  <production-state>production_ready</production-state>
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <vendor>Nuxeo</vendor>
  <license>LGPL 2.1</license>
  <license-url>http://www.gnu.org/licenses/lgpl-2.1.html</license-url>
  <visibility>MARKETPLACE</visibility>
</package>
